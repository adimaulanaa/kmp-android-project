CREATE TABLE user (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    age INTEGER NOT NULL
);

selectAllUsers:
SELECT * FROM user;

insertUser:
INSERT INTO user
VALUES (NULL, ?, ?);

updateUser:
UPDATE user
SET name = ?, age = ?
WHERE id = ?;

deleteAllUsers:
DELETE FROM user;

deleteUser:
DELETE FROM user
WHERE id = ?;